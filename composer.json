{
    "name": "octatechlib/octa-twinfield-connector",
    "type": "library",
    "description": "Library for using the Twinfield Soap Service with OAuth2.0.",
    "keywords": [
      "twinfield"
    ],
    "homepage": "https://github.com/octatechlib/octa-twinfield-connector",
    "license": "GPL-3.0-or-later",
    "authors": [
      {
        "name": "Octatech",
        "email": "office@octatech.nl"
      }
    ],
    "config": {
      "sort-packages": true
    },
    "conflict": {
      "pronamic/twinfield": "*"
    },
    "replace": {
      "pronamic/twinfield": "*"
    },
    "require": {
      "php": ">=7.3",
      "ext-dom": "*",
      "ext-json": "*",
      "ext-soap": "*",
      "moneyphp/money": "^3.0 || ^4.0",
      "myclabs/php-enum": "^1.5",
      "psr/http-message": "^1.0",
      "psr/log": "^1.0",
      "webmozart/assert": "^1.2"
    },
    "suggest": {
      "league/oauth2-client": "Using OAuth2.0"
    },
    "autoload": {
      "psr-4": {
        "OctaTwinfieldConnector\\": "src/"
      }
    },
    "autoload-dev": {
      "classmap": [
        "tests/"
      ]
    },
    "require-dev": {
      "eloquent/liberator": "^2.0",
      "league/oauth2-client": "^2.2",
      "phpstan/phpstan": "^0.12.88",
      "phpunit/phpunit": "^9.5"
    }
  }
  